{
  "presets": [
    [
      "@babel/env",
      {
        "loose": true,
        "modules": false
      }
    ],
    "@babel/react"
  ],
  "include": "**/*.js",
  "exclude": "**/*.ts",
  "plugins": [
    "babel-plugin-transform-async-to-promises",
    [
      "babel-plugin-import",
      {
        "libraryName": "tantd",
        "libraryDirectory": "lib",
        "style": true
      },
      "tantd"
    ]
  ]
}
